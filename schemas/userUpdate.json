{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "TestMaster", 
            "password": "password", 
            "first_name": "Master", 
            "last_name": "Test", 
            "email": "testMaster@test.com", 
            "photo_url": "http://testmasterpic.com/123xyz" , 
            "is_admin": true
        }
    ],
    "properties": {
        "username": {
            "$id": "#/properties/handle",
            "type": "string",
            "minLength": 6,
            "title": "The handle schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "TestMaster"
            ]
        },
        "password": {
            "$id": "#/properties/name",
            "type": "string",
            "minLength": 6,
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "password"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Master"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Test"
            ]
        },
        "email": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "format": "email",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "testMaster@test.com"
            ]
        },
        "photo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "format": "uri",
            "title": "The photo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "http://testmasterpic.com/123xyz"
            ]
        },
        "is_admin": {
            "$id": "#/properties/is_admin",
            "type": "boolean",
            "title": "The is_admin schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                true
            ]
        }
    },
    "additionalProperties": true
}